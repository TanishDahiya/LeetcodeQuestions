//344. Reverse String

class Solution {
public:
    void reverseString(vector<char>& s) {
        
        //Two pointers method with swap
        //recursive method
        //swap method
        //stack method
        
        int i=0;
        int j=s.size()-1;
        
        while(i<=j){
            char temp=s[i];
            s[i]=s[j];
            s[j]=temp;
            i++;
            j--;
        }
        
    }
};
